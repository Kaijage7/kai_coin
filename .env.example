# KAI Coin Deployment Configuration
# Copy this file to .env and fill in your values

# Deployer private key (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# RPC URLs
AMOY_RPC_URL=https://rpc-amoy.polygon.technology
POLYGON_RPC_URL=https://polygon-rpc.com

# Polygonscan API key for contract verification
POLYGONSCAN_API_KEY=your_api_key_here

# Oracle addresses (Chainlink on Polygon Amoy)
CHAINLINK_ORACLE_ADDRESS=0x0000000000000000000000000000000000000000

# Backend configuration
DATABASE_URL=postgresql://kai_admin:kaipass123@localhost:5433/kai_main
REDIS_URL=redis://localhost:6380
BLOCKCHAIN_RPC=http://localhost:8545

# Gas reporting
REPORT_GAS=true
